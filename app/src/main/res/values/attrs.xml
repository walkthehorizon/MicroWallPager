<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerificationCodeInput">
        <attr name="box" format="integer" />
        <attr name="child_h_padding" format="dimension" />
        <attr name="child_v_padding" format="dimension" />
        <attr name="child_width" format="dimension" />
        <attr name="child_height" format="dimension" />
        <attr name="padding" format="dimension" />
        <attr name="box_bg_focus" format="reference" />
        <attr name="box_bg_normal" format="reference" />
        <attr name="inputType" format="string" />
    </declare-styleable>

    <!-- RoundWidget end -->

    <declare-styleable name="RippleItemView">
        <attr name="item_icon" format="reference" />
        <attr name="item_title" format="string" />
        <attr name="item_arrow_visible" format="boolean" />
        <attr name="item_divider_visible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DefaultToolbar">
        <attr name="tl_left_icon" format="reference" />
        <attr name="tl_left_tint" format="color" />
        <attr name="tl_right_icon" format="reference" />
        <attr name="tl_title" format="string" />
        <attr name="tl_right_text" format="string" />
        <attr name="tl_right_icon2" format="reference" />
        <attr name="tl_end_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ArcImageView">
        <attr name="arc_height" format="dimension" />
    </declare-styleable>
</resources>